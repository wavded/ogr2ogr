{
  "name": "ogr2ogr",
  "version": "7.0.0",
  "description": "ogr2ogr wrapper w/ multiple format support",
  "keywords": [
    "ogr2ogr",
    "stream",
    "proj4",
    "gdal"
  ],
  "author": "Marc Harter <wavded@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/wavded/ogr2ogr.git"
  },
  "homepage": "http://github.com/wavded/ogr2ogr",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup index.ts --format esm,cjs --dts --clean",
    "test": "vitest run",
    "lint": "tsc --noEmit && biome check .",
    "fix": "biome check --write ."
  },
  "dependencies": {
    "archiver": "^7.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/archiver": "^6.0.3",
    "@types/node": "^24.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  }
}
