{
  "name": "ogr2ogr",
  "version": "5.1.0",
  "description": "ogr2ogr wrapper w/ multiple format support",
  "keywords": [
    "ogr2ogr",
    "stream",
    "proj4",
    "gdal"
  ],
  "author": "Marc Harter <wavded@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/wavded/ogr2ogr.git"
  },
  "homepage": "http://github.com/wavded/ogr2ogr",
  "type": "module",
  "main": "./dist/ogr2ogr.cjs",
  "module": "./dist/ogr2ogr.js",
  "types": "./dist/ogr2ogr.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/ogr2ogr.d.ts",
        "default": "./dist/ogr2ogr.js"
      },
      "require": {
        "types": "./dist/ogr2ogr.d.cts",
        "default": "./dist/ogr2ogr.cjs"
      }
    }
  },
  "scripts": {
    "prepublishOnly": "vite build",
    "build": "vite build",
    "test": "vitest run --silent --coverage",
    "lint": "tsc --noEmit && eslint .",
    "fmt": "prettier --write .",
    "fmt-check": "prettier --check ."
  },
  "dependencies": {
    "archiver": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/archiver": "^6.0.2",
    "@types/geojson": "^7946.0.13",
    "@types/node": "^22.13.5",
    "@vitest/coverage-v8": "^3.0.7",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^4.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.7"
  },
  "engines": {
    "node": ">=16"
  }
}
